<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ADJLinks">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/adavanced_java_concepts"
            android:fontFamily="@font/aladin"
            android:textColor="@color/black"
            android:textSize="35sp"
            android:padding="20dp">
        </TextView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="25dp"
            android:layout_marginTop="40dp">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10dp">


                <androidx.cardview.widget.CardView
                    android:id="@+id/adjl1"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="20sp"
                    app:cardElevation="20dp"
                    android:padding="25dp"
                    app:cardBackgroundColor="#FFFFFF"
                    app:cardCornerRadius="25dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/bg32">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:padding="15sp"
                            android:text="@string/event_handling"
                            android:textColor="#000000"
                            android:textSize="25sp"
                            app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/adjl2"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="20sp"
            app:cardElevation="20dp"
            android:padding="25dp"
            app:cardCornerRadius="25dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg32">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15sp"
                    app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:text="@string/introduction_to_awt"
                    android:textColor="#000000"
                    android:textSize="25sp" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/adjl3"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="20sp"
            app:cardElevation="20dp"
            android:padding="25dp"
            app:cardCornerRadius="25dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg32">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15sp"
                    app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:text="@string/swing_component"
                    android:textColor="#000000"
                    android:textSize="25sp" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/adjl4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20sp"
            app:cardElevation="20dp"
            android:padding="25dp"
            app:cardCornerRadius="25dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg32">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15sp"
                    app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:text="JDBC Client/Server"
                    android:textColor="#000000"
                    android:textSize="25sp" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/adjl5"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="20sp"
            app:cardElevation="20dp"
            android:padding="25dp"
            app:cardCornerRadius="25dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg32">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15sp"
                    app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:text="@string/jdbc_drivers"
                    android:textColor="#000000"
                    android:textSize="25sp" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/adjl6"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="20sp"
            app:cardElevation="20dp"
            android:padding="25dp"
            app:cardCornerRadius="25dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg32">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15sp"
                    app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:text="@string/java_beans"
                    android:textColor="#000000"
                    android:textSize="25sp" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/adjl7"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="20sp"
            app:cardElevation="20dp"
            android:padding="25dp"
            app:cardCornerRadius="25dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15sp"
                    android:background="@drawable/bg32"
                    app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:text="@string/bean_developer_kit_bdk"
                    android:textColor="#000000"
                    android:textSize="25sp" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/adjl8"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="20sp"
            app:cardElevation="20dp"
            android:padding="25dp"
            app:cardCornerRadius="25dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15sp"
                    android:background="@drawable/bg32"
                    app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                    android:text="@string/remote_method_invocation"
                    android:textColor="#000000"
                    android:textSize="25sp" />

            </LinearLayout>

        </androidx.cardview.widget.CardView>


    </LinearLayout>

</androidx.cardview.widget.CardView>

    </LinearLayout>

    </ScrollView>